{
  "name": "katello-capsule",
  "version": "1.0.0",
  "author": "Katello",
  "summary": "Deploys and manages a Capsule",
  "license": "GPL-3.0+",
  "source": "https://github.com/Katello/puppet-capsule.git",
  "project_page": "https://github.com/Katello/puppet-capsule",
  "issues_url": "http://projects.theforeman.org/projects/katello/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0 < 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.2.0 < 5.0.0"},
    {"name":"puppet/extlib","version_requirement":">= 0.10.4 < 1.0.0"},
    {"name":"puppetlabs-apache","version_requirement":">= 1.0.0 < 2.0.0"},
    {"name":"puppetlabs-mongodb","version_requirement":">= 0.11.0 < 1.0.0"},
    {"name":"theforeman-foreman","version_requirement":">= 5.0.0 < 6.0.0"},
    {"name":"theforeman-foreman_proxy","version_requirement":">= 2.2.0 < 3.0.0"},
    {"name":"katello-common","version_requirement":">= 0.0.1 < 1.0.0"},
    {"name":"katello-pulp","version_requirement":">= 1.0.0 < 2.0.0"},
    {"name":"katello-qpid","version_requirement":">= 1.0.0 < 2.0.0"},
    {"name":"katello-crane","version_requirement":">= 0.1.0 < 1.0.0"},
    {"name":"katello-certs","version_requirement":">= 0.1.0 < 1.0.0"}
  ]
}
